const targetURL = &#34;https://xoyondo.com/mb/ife3ymi06hit14s&#34;;

// 执行跳转（仅当当前路径是 /message/）
function redirectIfOnMessagePage() {
    // 兼容 localhost:1313/message/ 和 retr0.xyz/message/
    const path = window.location.pathname.replace(/\/+$/, &#34;&#34;);
    if (path === &#34;/message&#34;) {
        window.open(targetURL, &#34;_blank&#34;);
    }
}

// 普通加载
document.addEventListener(&#34;DOMContentLoaded&#34;, function () {
    redirectIfOnMessagePage();
});

// PJAX 加载
document.addEventListener(&#34;pjax:complete&#34;, function () {
    redirectIfOnMessagePage();
});
&lt;/script&gt;
